.body-wrapper {
  /* position: fixed;
  top: 0;
  width: 100%;
  bottom: 70px;
  overflow: hidden; */
  width: 100%;
}
 .header{
  display: flex;
  /* height: 120px; */
  background: #fff;
  width: 100%;
}
 .header .left {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 30%;
}
.header .left image {
  display: inline-block;
  width: 70px;
  height: 70px;
  border-radius: 50%;
}
 .header .right {
  width: 60%;
  padding: 20px 0 20px 0;
}
 .header .right .name {
  font-weight: 700;

}
.header .right .txt {
  font-size: 14px;
  white-space: "pre";
  color: #666666;
}
.coach-pp {
  width: 704rpx;
  margin: 0 auto;
  padding: 10px 0 0 30rpx; 
  color: #666666;
}
.my-coach {
  /* padding: 20px 0 10px 60rpx;  */
  /* font-weight: 500; */
  /* font-size: 14px; */
  /* color: #333333; */
}
.search-wrapper {
  position: relative;
  padding: 0 24rpx;
  margin-top: 20rpx;
}
.search-wrapper .search-input {
  
  /* border-radius: 2px; */
  padding: 14rpx 30rpx;
  border-radius: 12rpx;
  border: 1px solid #e4e4e4;
  font-size: 28rpx;
  /* box-shadow: 0 0 4rpx 1rpx rgba(0.49, 0.49, 0.49, 0.2); */
  /* box-shadow: 0 0 10rpx 1rpx  rgba(0.49, 0.49, 0.49, 0.2) */
}
.search-wrapper .search-icon {
  position: absolute;
  right: 50rpx;
  top: 12rpx;
}
.coach-wrapper {
  /* margin-top: 10px; */
  padding: 0 24rpx;
  

}
.coach-wrapper .coach-item {
  display: flex;
  margin-top: 10px;
  align-items: center;
  height: 112px;
  /* border-radius: 4px; */
  border-radius: 12rpx;
  /* border: 1px solid #e4e4e4; */
  /* box-shadow: 0 0 4rpx 1rpx rgba(0.49, 0.49, 0.49, 0.2); */
  /* box-shadow: 0 0 10rpx 1rpx  rgba(0.49, 0.49, 0.49, 0.2) */
}
.coach-wrapper .coach-item .avator{
  display: flex;
  justify-content: center;
  align-items: center;
  width: 30%;
}
.coach-wrapper .coach-item .avator image {
  display: inline-block;
  width: 70px;
  height: 70px;
  border-radius: 50%;
}
.coach-wrapper .coach-item .name {
  display: flex;
  align-items: center;
  line-height: 24px;
  font-weight: 700;
  font-size: 18px;
  color: #333333;
  vertical-align: middle;
}
.coach-wrapper .coach-item .my-icon {
  display: inline-block;
  margin-left: 5px;
}
.coach-wrapper .coach-item .exprience {
  margin-top: 6px;
  font-size: 14px;
  color: #666666;
}
.none-Tips {
  padding: 72rpx 0;
  color: #808080;
}


