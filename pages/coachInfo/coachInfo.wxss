Page {
  height: 100%;
  width: 100%;
}

.coachInfo {
  position: relative;
  width: 95%;
  height:25%;
  margin-top: 34rpx;
  margin-left:auto;
  margin-right: auto;
  /* box-shadow: 0px 0px 10rpx 1rpx rgba(0,0,0,0.1);  */
  border-radius: 15rpx;
}

.coachInfo-part1 {
  position: relative;
  width:100%;
  height:75%;
}

.coachInfo-avater {
  position: absolute;
  width: 140rpx;
  height: 140rpx;
  left: 72rpx;
  top:50%;
  border-radius: 50%;
  transform:translate(0,-50%);
}

.coachInfo-nickname {
  position: absolute;
  left: 266rpx;
  top:55%;
  transform:translate(0,-180%);
  font-weight: 900;
}

.coachInfo-experience {
  position: absolute;
  left: 266rpx;
  top:55%;
  transform:translate(0,-80%);
  color:#7d7d7d;
  font-size: 26rpx;
}

.coachInfo-phone {
  position: absolute;
  left: 266rpx;
  top:60%;
  color:#7d7d7d;
  font-size: 26rpx;
}

.coachInfo-part2 {
  position: relative;
  width: 100%;
  height: 25%;
  text-align: center;
  border-top: 1px solid #e4e4e4;
}

.coachInfo-property {
  position: relative;
  line-height: 100%;
  top: 50%;
  transform: translate(0,-50%);
  color:#7d7d7d;
  font-size: 30rpx;
}

.profile {
  position: relative;
  width:  95%;
  margin: 20rpx auto;
  /* box-shadow: 0px 0px 10rpx 1rpx rgba(0,0,0,0.1);  */
  border-radius: 15rpx;
}

.profile-title {
  position: relative;
  height: 88rpx;
  line-height: 88rpx;
  border-bottom: 1px solid #e4e4e4;
  padding-left: 52rpx;
  color: #7d7d7d;
  font-size: 32rpx;
}

.profile-info {
  position: relative;
  width: 100%;
  height: 262rpx;
}

.profile-info-text {
  position: relative;
  width: 85%;
  margin: 0 auto;
  top: 50%;
  transform: translate(0,-50%);
  color: #7d7d7d;
  font-size: 32rpx;
}

.reserve {
  width: 706rpx;
  height: 86rpx;
  background-color: #169bd5;
  color: white;
  text-align: center;
  line-height: 86rpx;
  margin-left:auto;
  margin-right: auto; 
  border-radius: 15rpx;
}

.reserve-hover {
  background-color:red;
}

.myCourse {
  width: 706rpx;
  height: 86rpx;
  background-color: #169bd5;
  color: white;
  text-align: center;
  line-height: 86rpx;
  margin-top: 20rpx;
  margin-left:auto;
  margin-right: auto; 
  border-radius: 15rpx;
}

.test {
  width: 800rpx;
}

.reverse-button {
  margin-top: 20rpx;
  position:relative;
   
  background-color: #199ed8;
  width: 702rpx;
  color: #ffffff;
}

.reverse-button-hover {
  background-color: rgba(25, 158, 216, 0.8);
}
