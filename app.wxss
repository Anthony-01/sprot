/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

/* 自制作图标 */
@font-face {
    font-family: 'male-icon';
    src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
    font-weight: normal;
    font-style: normal;
}



[class^="my-icon-"], [class*=" my-icon-"] {
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'male-icon' !important;
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}


.my-icon-female:before {
  content: "\e900";
  border-radius: 50%;
  padding: 4;
  color: #ffffff;
  background: #ff4d94;
}
.my-icon-male:before {
  content: "\e901";
  border-radius: 50%;
  padding: 4;
  color: #ffffff;
  background: #06a4fc;
}
